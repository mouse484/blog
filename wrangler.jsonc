{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "blog",
  "main": "dist/_worker.js/index.js",
  "compatibility_date": "2025-12-12",
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  }
}
